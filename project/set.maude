mod SET is protecting INT .

	sorts Element Set .
	subsort Int < Element < Set .

	op none : -> Set [ctor] . 
	op __ : Set Set -> Set [ctor assoc comm id: none] . 

	op _in_ : Element Set -> Bool .

	var I : Element . var S : Set .

	eq I in I S = true .
	eq I in S = false [owise] .

endm

